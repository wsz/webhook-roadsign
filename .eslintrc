{
  "env": {
    "node": true,
    "es6": true
  },
  "parserOptions": {
    "ecmaVersion": 2017,
    "ecmaFeatures": {
      "experimentalObjectRestSpread": true
    }
  },
  "extends": [
    "airbnb-base"
  ],
  "rules": {
    "semi": ["error", "never"],
    "quote-props": ["error", "consistent"],
    "object-curly-newline": ["error",
      {
        "consistent": true
      }
    ]
  }
}